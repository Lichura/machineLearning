<p>calcular precio de la casa</p>
<p>metros</p>
<p>habitaciones</p>


<p>ingresar datos para alimentar machine learning</p>
<%= form_for @casa do |f| %>
	<%= f.label :precio %>
	<%= f.text_field :precio %><br>
	<%= f.label :metros %>
	<%= f.text_field :metros %><br>
	<%= f.label :habitacion %>
	<%= f.text_field :habitacion %><br>
	<%= f.submit %>
<% end %>

<% @casas.each do |casa| %>
	<%= casa.id %>
	<%= casa.precio %>
<% end %>